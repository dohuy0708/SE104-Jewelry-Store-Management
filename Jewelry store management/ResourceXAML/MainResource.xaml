<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodel="clr-namespace:Jewelry_store_management.VIEWMODEL"
                    xmlns:view="clr-namespace:Jewelry_store_management.VIEW"
                  >
    <viewmodel:BaseViewModel x:Key="BaseVM"></viewmodel:BaseViewModel>
    <viewmodel:TestViewModel x:Key="TestVM"></viewmodel:TestViewModel>
    <viewmodel:StartViewModel x:Key="StartVM"></viewmodel:StartViewModel>
    <viewmodel:MainWindowViewModel x:Key="MainWindowVM"></viewmodel:MainWindowViewModel>
    <DataTemplate DataType="{x:Type viewmodel:SignInViewModel}">
        <view:SignInView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:SignUpViewModel}">
        <view:SignUpView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:ForgetPasswordViewModel}">
        <view:ForgetPasswordView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:AddProductViewModel}">
        <view:AddProductView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:AddSupplierViewModel}">
        <view:AddSuppierView/>
    </DataTemplate>
</ResourceDictionary>