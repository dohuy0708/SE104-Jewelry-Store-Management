        </StackPanel>
        <StackPanel Margin="60 10 0 0">
            <Label Content="TÊN NHÀ CUNG CẤP"></Label>
            <TextBox Margin="0 0 60 0"></TextBox>
        </StackPanel>
        <StackPanel Margin="60 10 0 0">
            <Label Content="SỐ ĐIỆN THOẠI"></Label>
            <TextBox Margin="0 0 60 0"></TextBox>
        </StackPanel>
        <StackPanel Margin="60 10 0 0">
            <Label Content="ĐỊA CHỈ"></Label>
            <TextBox Margin="0 0 60 0" Height="71"></TextBox>
        </StackPanel>
        <Button Margin="60 20 60 0" Content="THÊM NHÀ CUNG CẤP" Background="Aqua" BorderBrush="Transparent"></Button>
